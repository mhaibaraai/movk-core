{
  "name": "@movk/core",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.23.0",
  "description": "一个为现代 Vue.js 应用量身打造的高性能实用工具与组合式函数集合。",
  "author": "YiXuan <mhaibaraai@gmail.com>",
  "license": "MIT",
  "homepage": "https://core.mhaibaraai.cn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-core.git"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-core/issues"
  },
  "keywords": [
    "movk",
    "@movk/core",
    "tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "cd docs && nr prepare",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "docs": "cd docs && nr dev",
    "docs:build": "cd docs && nr build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "nr build",
    "release": "release-it && npm publish --access public",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit && cd docs && nr typecheck",
    "test": "vitest",
    "clean": "tsx scripts/rm.ts"
  },
  "peerDependencies": {
    "vue": "^3.5.24"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@antfu/ni": "^27.0.1",
    "@release-it/conventional-changelog": "^10.0.2",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "release-it": "^19.0.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.13"
  }
}
