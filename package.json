{
  "name": "@movk/core",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@10.30.1",
  "description": "为 TypeScript 项目设计的现代化、支持 Tree-Shaking 的工具函数库。",
  "author": "YiXuan <mhaibaraai@gmail.com>",
  "license": "MIT",
  "homepage": "https://core.mhaibaraai.cn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-core.git"
  },
  "bugs": {
    "url": "https://github.com/mhaibaraai/movk-core/issues"
  },
  "keywords": [
    "movk",
    "@movk/core",
    "tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "movk-clean": "./bin/clean.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "docs": "cd docs && pnpm dev",
    "docs:build": "cd docs && pnpm build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm build",
    "release": "release-it && npm publish",
    "typecheck": "tsc --noEmit && cd docs && pnpm typecheck",
    "test": "vitest",
    "clean": "node bin/clean.mjs"
  },
  "peerDependencies": {
    "vue": "^3.5.25"
  },
  "dependencies": {
    "@vueuse/core": "^14.2.1",
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/node": "^25.3.0",
    "eslint": "^9.39.3",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18"
  }
}
